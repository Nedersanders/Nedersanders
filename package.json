{
  "name": "sanderfest",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "npm run build:css && PORT=3333 nodemon ./bin/www",
    "build:css": "postcss public/stylesheets/style.css -o public/stylesheets/output.css",
    "watch:css": "postcss public/stylesheets/style.css -o public/stylesheets/output.css --watch"
  },
  "dependencies": {
    "@sentry/node": "^9.42.0",
    "@tailwindcss/postcss": "^4.1.11",
    "autoprefixer": "^10.4.21",
    "cookie-parser": "~1.4.4",
    "debug": "~4.4.1",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "http-errors": "~2.0.0",
    "morgan": "^1.10.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
