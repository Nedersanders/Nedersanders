{
  "name": "sanderfest",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build:prod && node ./bin/www",
    "dev": "npm run build && nodemon ./bin/www",
    "build": "npm run build:css && rollup -c",
    "build:watch": "npm run build:css && rollup -c --watch",
    "build:prod": "npm run build:css && NODE_ENV=production rollup -c",
    "build:css": "postcss public/stylesheets/style.css -o public/stylesheets/output.css",
    "watch:css": "postcss public/stylesheets/style.css -o public/stylesheets/output.css --watch"
  },
  "dependencies": {
    "@sentry/node": "^9.42.0",
    "@sentry/profiling-node": "^9.42.0",
    "@tailwindcss/postcss": "^4.1.11",
    "autoprefixer": "^10.4.21",
    "cookie-parser": "~1.4.4",
    "debug": "~4.4.1",
    "dotenv": "^17.2.1",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "http-errors": "~2.0.0",
    "morgan": "^1.10.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "rollup": "^4.0.0",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup-plugin-postcss": "^4.0.2",
    "postcss-import": "^16.0.0",
    "cssnano": "^6.0.1"
  }
}
